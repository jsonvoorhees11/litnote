{
  "DbSettings": {
    "NotesCollectionName": "notes",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "litnote"
  },
  "GitHubOAuthSettings": {
    "OAuthClientId": "$abc",
    "OAuthClientSecret": "$abc",
    "AuthorizeUrl": "https://github.com/login/oauth/authorize",
    "TokenGenerateUrl": "https://github.com/login/oauth/access_token",
    "GitHubUserInfoUrl": "https://api.github.com/user"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
